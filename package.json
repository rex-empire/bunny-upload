{
  "name": "npm-module-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for npm modules with ES6 features and syntax",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env BABEL_ENV=production babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "Dinesh Pandiyan <flexdinesh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flexdinesh/npm-module-boilerplate/issues"
  },
  "homepage": "https://github.com/flexdinesh/npm-module-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "es6-promise-pool": "^2.5.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.6",
    "path": "^0.12.7"
  }
}
